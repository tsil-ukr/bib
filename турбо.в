взяти визначення ./Виділяч;

секція турбо {
  структура Турбіна {
    виділяч: адреса<біб::Виділяч>;
  }

  структура ІнтернетОбслуговувач;
  структура ІнтернетЗвʼязок;
  структура Помилка;

  синонім ВідкликТурбіни = (турбіна: адреса<Турбіна>, аргумент: невідома_адреса) -> ніщо;

  синонім ОбробникПідключенняІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса) -> ніщо;
  синонім ОбробникВідключенняІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса, помилка: позитивне) -> ніщо;
  синонім ОбробникДанихІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса, розмір: позитивне, дані: памʼять<п8>) -> ніщо;
  синонім ОбробникЗакриттяІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса) -> ніщо;
  синонім ОбробникЗаписуІнтернетЗвʼязку = (турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент_інтернет_звʼязку: невідома_адреса, аргумент_запису: невідома_адреса, розмір: позитивне, дані: памʼять<п8>) -> ніщо;
  синонім ОбробникЗапускуІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, аргумент: невідома_адреса, іа: позитивне, порт: позитивне) -> ніщо;
  синонім ОбробникПідключенняКлієнтаІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, аргумент: невідома_адреса) -> ніщо;
  синонім ОбробникЗупинкиІнтернетОбслуговувача = (турбіна: адреса<Турбіна>, інтернет_обслуговувач: адреса<ІнтернетОбслуговувач>, аргумент: невідома_адреса) -> ніщо;
}

зовнішня дія __турбо__створити_турбіну(виділяч: адреса<біб::Виділяч>): адреса<турбо::Турбіна>;
зовнішня дія __турбо__запустити_турбіну(турбіна: адреса<турбо::Турбіна>, відклик_перед: турбо::ВідкликТурбіни, відклик_після: турбо::ВідкликТурбіни, аргумент: невідома_адреса);
зовнішня дія __турбо__знищити_турбіну(турбіна: адреса<турбо::Турбіна>);

зовнішня дія __турбо__отримати_інтернет_адресу_з_ю8(турбіна: адреса<турбо::Турбіна>, значення: ю8, вихід: адреса<позитивне>): логічне;

зовнішня дія __турбо__підключити_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, іа: позитивне, порт: позитивне, обробник_підключення: турбо::ОбробникПідключенняІнтернетЗвʼязку, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса): логічне;
зовнішня дія __турбо__записати_в_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, розмір: позитивне, дані: памʼять<п8>, обробник_запису: турбо::ОбробникЗаписуІнтернетЗвʼязку, аргумент: невідома_адреса);
зовнішня дія __турбо__закрити_інтернет_звʼязок(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>);
зовнішня дія __турбо__встановити_обробники_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса);

зовнішня дія __турбо__запустити_інтернет_обслуговувач(турбіна: адреса<турбо::Турбіна>, іа: позитивне, порт: позитивне, обробник_запуску: турбо::ОбробникЗапускуІнтернетОбслуговувача, обробник_підключення_клієнта: турбо::ОбробникПідключенняКлієнтаІнтернетОбслуговувача, обробник_зупинки: турбо::ОбробникЗупинкиІнтернетОбслуговувача, аргумент: невідома_адреса): логічне;

секція турбо {
  дія створити_турбіну(виділяч: адреса<біб::Виділяч>): адреса<Турбіна> {
    вернути __турбо__створити_турбіну(виділяч);
  }

  дія запустити_турбіну(турбіна: адреса<Турбіна>, відклик_перед: турбо::ВідкликТурбіни, відклик_після: турбо::ВідкликТурбіни, аргумент: невідома_адреса) {
    __турбо__запустити_турбіну(турбіна, відклик_перед, відклик_після, аргумент);
  }

  дія знищити_турбіну(турбіна: адреса<Турбіна>) {
    __турбо__знищити_турбіну(турбіна);
  }

  дія отримати_інтернет_адресу_з_ю8(турбіна: адреса<турбо::Турбіна>, значення: ю8, вихід: адреса<позитивне>): логічне {
    вернути __турбо__отримати_інтернет_адресу_з_ю8(турбіна, значення, вихід);
  }

  дія підключити_інтернет_звʼязок(турбіна: адреса<Турбіна>, іа: позитивне, порт: позитивне, обробник_підключення: ОбробникПідключенняІнтернетЗвʼязку, обробник_даних: ОбробникДанихІнтернетЗвʼязку, обробник_закриття: ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса): логічне {
    вернути __турбо__підключити_інтернет_звʼязок(турбіна, іа, порт, обробник_підключення, обробник_даних, обробник_закриття, обробник_відключення, аргумент);
  }

  дія записати_в_інтернет_звʼязок(турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>, розмір: позитивне, дані: памʼять<п8>, обробник_запису: ОбробникЗаписуІнтернетЗвʼязку, аргумент: невідома_адреса) {
    __турбо__записати_в_інтернет_звʼязок(турбіна, інтернет_звʼязок, розмір, дані, обробник_запису, аргумент);
  }

  дія встановити_обробники_інтернет_звʼязку(турбіна: адреса<турбо::Турбіна>, інтернет_звʼязок: адреса<турбо::ІнтернетЗвʼязок>, обробник_даних: турбо::ОбробникДанихІнтернетЗвʼязку, обробник_закриття: турбо::ОбробникЗакриттяІнтернетЗвʼязку, обробник_відключення: турбо::ОбробникВідключенняІнтернетЗвʼязку, аргумент: невідома_адреса) {
    __турбо__встановити_обробники_інтернет_звʼязку(турбіна, інтернет_звʼязок, обробник_даних, обробник_закриття, обробник_відключення, аргумент);
  }

  дія закрити_інтернет_звʼязок(турбіна: адреса<Турбіна>, інтернет_звʼязок: адреса<ІнтернетЗвʼязок>) {
    __турбо__закрити_інтернет_звʼязок(турбіна, інтернет_звʼязок);
  }

  дія запустити_інтернет_обслуговувач(турбіна: адреса<Турбіна>, іа: позитивне, порт: позитивне, обробник_запуску: ОбробникЗапускуІнтернетОбслуговувача, обробник_підключення_клієнта: ОбробникПідключенняКлієнтаІнтернетОбслуговувача, обробник_зупинки: ОбробникЗупинкиІнтернетОбслуговувача, аргумент: невідома_адреса): логічне {
    вернути __турбо__запустити_інтернет_обслуговувач(турбіна, іа, порт, обробник_запуску, обробник_підключення_клієнта, обробник_зупинки, аргумент);
  }
}